<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="CONSOLE" target="System.out" ignoreExceptions="true">
            <PatternLayout pattern="[%d{dd HH:mm:ss,SSS} %p %t %c{2}] %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="TRACE">
            <Appender-Ref ref="CONSOLE"/>
        </Root>
    </Loggers>
</Configuration>